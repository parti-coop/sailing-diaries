= render 'pages/dashboard_style', month: 7
= render 'pages/dashboard_style', month: 8
= render 'pages/dashboard_style', month: 9
= render 'pages/dashboard_style', month: 10
%section#dashboard-section
  .graph.slideshow-container
    .title.pull-right
      %div{style:"margin:40px 0px;"}
        Sailing
        %br
        Diary
        %br
        %span{style:"display:block;margin-top:-10px;"}
          A month
    .color-pallet
      %table
        %tr
          %td.color-box{style:"background-color: #fa8dbe"}
          %td
            Joy
        %tr
          %td.color-box{style:"background-color: #faaa94"}
          %td
            Happy
        %tr
          %td.color-box{style:"background-color: #fcedb7"}
          %td
            Satisfied
        %tr
          %td.color-box{style:"background-color: #90e3d5"}
          %td
            Fun
        %tr
          %td.color-box{style:"background-color: #79d7fc"}
          %td
            Depressed
        %tr
          %td.color-box{style:"background-color: #7b78eb"}
          %td
            Exhausted
        %tr
          %td.color-box{style:"background-color: #d362e8"}
          %td
            Annoyed
        %tr
          %td.color-box{style:"background-color: #cfb0e9"}
          %td
            Nervous

    #js-slideshow
      .multi-repeating-linear-7
        .period-box.text-center
          2017.07.01 ~ 2017.07.31
      .multi-repeating-linear-8
        .period-box.text-center
          2017.08.01 ~ 2017.08.31
      .multi-repeating-linear-9
        .period-box.text-center
          2017.09.01 ~ 2017.09.30
      .multi-repeating-linear-10
        .period-box.text-center
          2017.10.01 ~ 2017.10.27
  .feed
    %h3
      How was your day?
    .show-item
      .card
        .card-item
          = render 'items/show', item: @item
        .card-item
          = render 'items/show', item: @item2
