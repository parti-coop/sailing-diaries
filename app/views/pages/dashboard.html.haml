= render 'pages/dashboard_style', month: 7
= render 'pages/dashboard_style', month: 8
= render 'pages/dashboard_style', month: 9
= render 'pages/dashboard_style', month: 10
%section#dashboard-section
  .graph.slideshow-container
    .title.pull-right
      %div{style:"margin:40px 0px;"}
        Sailing
        %br
        Diary
        %br
        %span{style:"display:block;margin-top:-10px;"}
          A month
    #js-slideshow
      .multi-repeating-linear-7
        .period-box.text-center
          2017.07.01 ~ 2017.07.31
      .multi-repeating-linear-8
        .period-box.text-center
          2017.08.01 ~ 2017.08.31
      .multi-repeating-linear-9
        .period-box.text-center
          2017.09.01 ~ 2017.09.30
      .multi-repeating-linear-10
        .period-box.text-center
          2017.10.01 ~ 2017.10.27
  .feed
    %h3
      how was your day?
    .show-item
      .card
        .card-item
          = render :partial => 'items/show', :locals => {:item => Item.find(4000)}
        .card-item
          = render :partial => 'items/show', :locals => {:item => Item.find(3989)}
